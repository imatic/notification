{
    "require": {
        "videlalvaro/php-amqplib": "~2.4",
        "symfony/options-resolver": "~2.4",
        "psr/log": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "gonzalo123/yaml2pimple": "*"
    },
    "suggest": {
        "gonzalo123/yaml2pimple": "Create connection object for you",
        "symfony/symfony": "Great framework with container able to create connection object for you"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imatic\\Notification\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
